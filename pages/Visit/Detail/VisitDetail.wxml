<!--pages/Visit/Detail/VisitDetail.wxml-->
<view class="page" xmlns:wx="http://www.w3.org/1999/xhtml">
<view  class="weui-cells__title">
拜访详情
</view>
<view>
<view  class="weui-cells weui-cells_after-title">
<view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">
                    <view class="weui-label">拜访主题</view>
                </view>
                <view class="weui-cell__bd">
                  <input class="weui-input weui-input_font_right" value='{{visitEntity.visit_Objective}}'/>
                </view>
            </view>
          <view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">
                    <view class="weui-label">客户编号</view>
                </view>
                <view class="weui-cell__bd">
                  <input class="weui-input weui-input_font_right" value='{{visitEntity.customer_Code}}'/>
                </view>
            </view>
<view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">
                    <view class="weui-label">客户名称</view>
                </view>
                <view class="weui-cell__bd">
                  <input class="weui-input weui-input_font_right" value='{{visitEntity.customer_Name}}'/>
                </view>
            </view>
<view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">
                    <view class="weui-label">客户地址</view>
                </view>
                <view class="weui-cell__bd">
                  <input class="weui-input weui-input_font_right" value='{{visitEntity.customer_Address}}'/>
                </view>
            </view>
            <view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">
                    <view class="weui-label">计划拜访时间</view>
                </view>
                <view class="weui-cell__bd">
                  <input class="weui-input weui-input_font_right" value='{{visitEntity.planning_Time}}'/>
                </view>
            </view>
            <view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">
                    <view class="weui-label">实际拜访时间</view>
                </view>
                <view class="weui-cell__bd">
                  <input class="weui-input weui-input_font_right" value='{{visitEntity.actual_Time}}'/>
                </view>
            </view>
            <view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">
                    <view class="weui-label">同行人</view>
                </view>
                <view class="weui-cell__bd">
                  <input class="weui-input weui-input_font_right" value='{{visitEntity.peer_Personnel}}'/>
                </view>
            </view>
                  <view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">
                    <view class="weui-label">交通工具</view>
                </view>
                <view class="weui-cell__bd">
                   <picker data-name='Vehicle' bindchange="vehicleChange" value="{{visitEntity.vehicle}}" range="{{vehicleList}}" >
                 <view class="weui-select weui-input weui-input_font_right">{{visitEntity.vehicle}}</view>
                 </picker>
                </view>
            </view>
               <view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">
                    <view class="weui-label">拜访地点</view>
                </view>
                <view class="weui-cell__bd">
      <input class="weui-input weui-input_font_right" value='{{visitEntity.visit_Address}}'/>
                </view>
            </view>
           <view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">
                    <view class="weui-label">备注</view>
                </view>
                <view class="weui-cell__bd">
      <input data-name='Remark' bindinput="visInput" class="weui-input weui-input_font_right" value='{{visitEntity.Remark}}'/>
                </view>
            </view>
    </view>
</view>
</view>
